                                tstr	macro	insn,memop,hliy,hlix,hl,de,bc,flags,acc,sp
                                	local	lab
                                lab:	db	insn
                                	ds	lab+4-$,0
                                	dw	memop,hliy,hlix,hl,de,bc
                                	db	flags
                                	db	acc
                                	dw	sp
                                	if	$-lab ne 20
                                	error	'missing parameter'
                                	endif
                                	endm
                                
                                tmsg	macro	m
                                	local	lab
                                lab:	db	m
                                	if	$ ge lab+30
